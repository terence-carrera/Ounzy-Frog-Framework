APP_NAME="Frog Web Application"
APP_VERSION=0.1.0
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost:8000
API_TOKEN=secret
FROG_BASE_URL=

# Session
SESSION_NAME=FROGSESSID
SESSION_LIFETIME=120
SESSION_PATH=/
SESSION_DOMAIN=
SESSION_SECURE=false
SESSION_HTTPONLY=true
SESSION_SAMESITE=Lax

# CSRF
CSRF_ENABLED=true
CSRF_TOKEN_KEY=_csrf_token
CSRF_HEADER=X-CSRF-TOKEN
CSRF_FIELD=_token

# Database
DB_CONNECTION=sqlite
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=

# Cache
CACHE_DRIVER=file
CACHE_PATH=

# Mail
MAIL_DRIVER=mail
MAIL_FROM_ADDRESS=noreply@example.com
MAIL_FROM_NAME=Frog
MAIL_HOST=127.0.0.1
MAIL_PORT=587
MAIL_ENCRYPTION=tls
MAIL_USERNAME=
MAIL_PASSWORD=

# Keycloak
KEYCLOAK_ENABLED=false
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=master
KEYCLOAK_CLIENT_ID=
KEYCLOAK_CLIENT_SECRET=
KEYCLOAK_JWKS_TTL=3600
KEYCLOAK_ISSUER=
